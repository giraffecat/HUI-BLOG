       <el-submenu :key=\"route.path\" :index=\"route.path\">\n            <!-- 二级菜单标题 -->\n            <template slot=\"title\">\n              <i :class=\"route.icon\" />\n              <span>{{ route.name }}</span>\n            </template>\n            <!-- 二级菜单选项 -->\n            <template v-for=\"(item, index) of route.children\">\n              <el-menu-item v-if=\"!item.hidden\" :key=\"index\" :index=\"item.path\">\n                <i :class=\"item.icon\" />\n                <span slot=\"title\">{{ item.name }}</span>\n              </el-menu-item>\n            </template>\n          </el-submenu>\n        </template>\n        <!-- 一级菜单 -->\n        <template v-else-if=\"!route.is_hidden\">\n          <el-menu-item :index=\"route.path\" :key=\"route.path\">\n            <i :class=\"route.icon\" />\n            <span slot=\"title\">{{ route.name }}</span>\n          </el-menu-item>\n        </template>\n      </template>\n    </el-menu>\n  </div>\n</template>\n<script>\nexport default {\n  created() {\n    console.log(\"sideBaras\", this.$store.state.userMenuList)\n  }\n}\n</script>\n<style scoped>\n.side-nav-bar:not(.el-menu--collapse) {\n  width: 210px;\n}\n.side-nav-bar {\n  position: fixed;\n  top: 0;\n  left: 0;\n  bottom: 0;\n  overflow-x: hidden;\n  overflow-y: auto;\n}\n.side-nav-bar i {\n  margin-right: 1rem;\n}\n*::-webkit-scrollbar {\n  width: 0.5rem;\n  height: 1px;\n}\n*::-webkit-scrollbar-thumb {\n  border-radius: 0.5rem;\n  background-color: rgba(144, 147, 153, 0.3);\n}\n</style>\n"]}]}